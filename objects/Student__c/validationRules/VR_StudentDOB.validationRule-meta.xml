<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_StudentDOB</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Date_of_Birth__c &gt; TODAY(),YEAR(Date_of_Birth__c) &lt; 1990 )</errorConditionFormula>
    <errorDisplayField>Date_of_Birth__c</errorDisplayField>
    <errorMessage>DOB should not be before 1990 and future dates.</errorMessage>
</ValidationRule>
